{
  "name": "gear-rental-app",
  "version": "1.0.0",
  "description": "Cinema Equipment Rental Management System with AI Recommendations",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node scripts/seedData.js",
    "ai:start": "cd ai-service && python app.py",
    "ai:dev": "cd ai-service && python app.py",
    "dev:all": "concurrently \"npm run dev\" \"npm run ai:dev\"",
    "test:ai": "curl http://localhost:5001/health",
    "test:backend": "curl http://localhost:5000/health",
    "test:ai-connection": "curl http://localhost:5000/api/ai/test-connection",
    "setup:ai": "cd ai-service && pip install -r requirements.txt"
  },
  "keywords": [
    "cinema",
    "equipment",
    "rental",
    "nodejs",
    "express",
    "mongodb",
    "ai",
    "recommendations"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.7",
